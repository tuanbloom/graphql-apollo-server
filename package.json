{
  "name": "@makerx/graphql-apollo-server",
  "version": "1.0.0",
  "private": false,
  "description": "A set of MakerX plugins for Apollo Server",
  "author": "MakerX",
  "license": "MIT",
  "engines": {
    "node": ">=16.0"
  },
  "bugs": {
    "url": "https://github.com/MakerXStudio/graphql-apollo-server/issues"
  },
  "homepage": "https://github.com/MakerXStudio/graphql-apollo-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MakerXStudio/graphql-apollo-server.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:0-clean": "rimraf dist coverage",
    "build:1-compile": "tsc -p tsconfig.build.json",
    "build:2-copy-pkg-json": "npx --yes @makerx/ts-toolkit@latest copy-package-json",
    "test": "jest --passWithNoTests --coverage --coverageReporters=text",
    "lint": "eslint ./src/ --ext .ts",
    "lint:fix": "eslint ./src/ --ext .ts --fix",
    "check-types": "tsc --noEmit",
    "audit": "better-npm-audit audit"
  },
  "dependencies": {
    "@makerx/graphql-core": "^1.0.0",
    "@makerx/node-common": "^1.0.3"
  },
  "peerDependencies": {
    "@apollo/server": "4 || 5",
    "graphql": "*"
  },
  "devDependencies": {
    "@makerx/ts-config": "^1.0.1",
    "@tsconfig/node16": "^16.1.0",
    "@types/jest": "^29.5.2",
    "@types/lodash.omit": "^4.5.7",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "better-npm-audit": "^3.7.3",
    "copyfiles": "^2.4.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "semver": "^7.5.3"
  }
}
